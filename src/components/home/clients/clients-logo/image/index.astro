---

// IMPORTS ASTRO ATOMS
import ContainLeftPicture from "@/atoms/picture/contain/left/index.astro"

// GET ASTRO PROPS
const {

    alternative_text,
    CDN,
    image_width,
    PROJECT,
    source

} = Astro.props
---
<div class="w-full h-12">
    <ContainLeftPicture
        alt={ alternative_text }
        CDN={ CDN }
        image_width={ image_width }
        PROJECT={ PROJECT }
        src={ source }

    />
</div>
